<?xml version="1.0"?>
<odoo>
    <data>
        

        <record model="ir.actions.act_window" id="session_list_action">
            <field name="name">Request for Measurement</field>
            <field name="res_model">philsteel.amrequests</field>
            <field name="res_model">philsteel.materials</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,calendar,gantt,graph</field>
        </record>

        
        <record model="ir.ui.view" id="field_graph_view">
            <field name="name">philsteel.amrequests.graph</field>
            <field name="model">philsteel.amrequests</field>
            <field name="arch" type="xml">
                <graph string="Status of Request of Measurement by their types" orientation="horizontal" type="bar">
                    <field name="assignedTo"/>
                    <field name="statuss" type="bar"/>
                </graph>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_field_graph">
            <field name="name">Field</field>
            <field name="res_model">philsteel.amrequests</field>
            <field name="view_type">form</field>
            <field name="view_mode">graph</field>
            <field name="view_id"
                   ref="field_graph_view"/>
        </record>


        <record model="ir.ui.view" id="pis_graph_view">
            <field name="name">philsteel.pisreports.graph</field>
            <field name="model">philsteel.pisreports</field>
            <field name="arch" type="xml">
                <graph string="Request of Measurement Assigned to the Users">
                    <field name="assignedTo" />
                    <field name="statuss" />
                </graph>
            </field>

        </record>

        <record model="ir.actions.act_window" id="act_pis_graph">
            <field name="name">Request of Measurement Assigned to the Users</field>
            <field name="res_model">philsteel.pisreports</field>
            <field name="view_type">form</field>
            <field name="view_mode">graph</field>
            <field name="view_id"
                   ref="pis_graph_view"/>
        </record>


        <record model="ir.ui.view" id="infosheet_graph_view">
            <field name="name">philsteel.infosheet.graph</field>
            <field name="model">philsteel.infosheet</field>
            <field name="arch" type="xml">
                <graph string="Request of Measurement Assigned to the Users">
                    <field name="assignedTo" />
                    <field name="statuss" />
                </graph>
            </field>

        </record>

        <record model="ir.actions.act_window" id="act_infosheet_graph">
            <field name="name">Request of Measurement Assigned to the Users</field>
            <field name="res_model">philsteel.infosheet</field>
            <field name="view_type">form</field>
            <field name="view_mode">graph</field>
            <field name="view_id"
                   ref="infosheet_graph_view"/>
        </record>

        <record model="ir.ui.view" id="pas_graph_view">
            <field name="name">philsteel.pas.graph</field>
            <field name="model">philsteel.pas</field>
            <field name="arch" type="xml">
                <graph string="Request of Measurement Assigned to the Users">
                    <field name="assignedTo" />
                    <field name="statuss" />
                </graph>
            </field>

        </record>

        <record model="ir.actions.act_window" id="act_pas_graph">
            <field name="name">Request of Measurement Assigned to the Users</field>
            <field name="res_model">philsteel.pas</field>
            <field name="view_type">form</field>
            <field name="view_mode">graph</field>
            <field name="view_id"
                   ref="pas_graph_view"/>
        </record>


        <record model="ir.actions.act_window" id="act_request_list">
            <field name="name">Request for Measurement</field>
            <field name="res_model">philsteel.amrequests</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        

        <record model="ir.ui.view" id="board_session_form">
            <field name="name">Session Dashboard Form</field>
            <field name="model">board.board</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Session Dashboard">
                    <board style="3-3">
                        <column>
                            
                            <action
                                string="Status of Request of Measurement assigned to Field Engineers"
                                name="%(act_field_graph)d"
                                height="350"
                                width="510"/>
                            <action
                                string="Status of Inspection and Safety Report assigned to Field Engineers"
                                name="%(act_pis_graph)d"
                                height="350"
                                width="510"/>
                            <action
                                string="Status of Project Information assigned to Field Engineers"
                                name="%(act_infosheet_graph)d"
                                height="350"
                                width="510"/>
                            <action
                                string="Status of Project Accomplishment Report assigned to Field Engineers"
                                name="%(act_pas_graph)d"
                                height="350"
                                width="510"/>
                        </column>
                      
                    </board>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="open_board_session">
          <field name="name">Session Dashboard</field>
          <field name="res_model">board.board</field>
          <field name="view_type">form</field>
          <field name="view_mode">form</field>
          <field name="usage">menu</field>
          <field name="view_id" ref="board_session_form"/>
        </record>
    </data>
</odoo>